{
  "name": "sacastats",
  "version": "1.0.0",
  "description": "A website for SaCa outfit member stats in PlanetSide 2.",
  "main": "../priv/static/assets/dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "node build.js",
    "test": "jest --config jest.config.js",
    "format": "prettier --write \"src/**/*.ts\"",
