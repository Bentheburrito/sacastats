{
  "name": "sacastats",
  "version": "1.0.0",
  "description": "A website for SaCa outfit member stats in PlanetSide 2.",
  "main": "../priv/static/assets/dist/scripts.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf ../priv/static/assets/dist && node --experimental-json-modules build-production.js",
    "buildDev": "rm -rf ../priv/static/assets/dist && node --experimental-json-modules build-dev.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run lint && npm test",
    "preversion": "npm run lint && npm test",
    "prepare": "npm run build",
    "save": "npm run lint && npm run format && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bentheburrito/sacastats.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bentheburrito/sacastats/issues"
  },
  "homepage": "https://github.com/Bentheburrito/sacastats#readme",
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "@types/three": "^0.146.0",
    "bootstrap-table": "^1.21.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jquery": "^3.6.1",
    "npm-dts": "^1.3.12",
    "prettier": "^2.7.1",
    "string.prototype.replaceall": "^1.0.7",
    "three": "^0.146.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {},
  "files": [
    "../priv/static/assets/dist/*"
  ],
  "type": "module",
  "dependencies": {
    "esbuild": "^0.15.13"
  }
}